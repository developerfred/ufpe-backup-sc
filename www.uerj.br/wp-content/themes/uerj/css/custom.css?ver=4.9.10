    top: calc(30px + 1.5rem);
    width: 0;
}

.publicacoes__revista figure:before{
    /*transition: all 0.5s ease;*/
    content:'';
    height: 1px;
    position: absolute;
    left: 15px;
    background-color: rgba(255,255,255,0.35);
    z-index: 9;
    top: calc(30px + 1.5rem);
