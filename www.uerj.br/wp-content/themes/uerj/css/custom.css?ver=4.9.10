}

.menu-principal-navegacao > .menu-item > .dropdown-menu .menu-item.active > a{
    /*border-left:#bf9000 solid 2px;*/
    /*padding-left: 8px;*/
    /*background-color: rgba(4,89,135,0.5);*/
