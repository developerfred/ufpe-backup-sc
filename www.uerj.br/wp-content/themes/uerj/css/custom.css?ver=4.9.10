.page-wrap > .slideshow{
    background-color: #ccc;
    height: 395px;
    margin: 0;
    padding: 0;
    position: relative;
    overflow: hidden;
    top: 90px;

}
.slideshow h2{
    color: #fff;
    text-align: right;
    font-size: 3.5rem;
    position: absolute;
    bottom: 30px;
    font-weight: 400;
    /*text-shadow: 0 0 5px rgba(0,0,0,0.5);*/
